    </div>
    <div class="tab" id="monomers">
      <ul class="pages"></ul>
      <div clas="data"><h1>No data</h1></div>
      <ul class="pages"></ul>
    </div>
    <div class="tab" id="others">
      <ul class="pages"></ul>
      <div clas="data"><h1>No data</h1></div>
      <ul class="pages"></ul>
    </div>
    <div class="tab" id="stats">
      <div class="data"><h1>No data</h1></div>
    </div>


