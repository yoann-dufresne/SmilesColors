<!DOCTYPE html>

<html>
  <head>
    <meta charset="utf-8">
    <title>Results</title>
    <link rel="stylesheet" type="text/css" href="style.css" media="screen" />
    <link rel="icon" type="image/x-icon" href="imgs/icon.ico">
  </head>

  <body>
    <ul id="menu">
      <li id="add">+</li>
      <li class="tabButton" id="0">Peptides</li>
      <li class="tabButton" id="1">Monomers</li>
      <li class="tabButton" id="2">Others</li>
      <li class="tabButton" id="3">General</li>
    </ul>

    <div id="addZip">
      <p>Add results zip files</p>
      <input type="file" id="files" name="files[]" multiple accept=".zip"/>
      <div id="fileslist"></div>
      <button id="okAdd">ok</button>
    </div>

    <!--input type="file" id="files" name="files[]" multiple accept=".zip"/-->

    <div class="tab" id="peptides">
      <ul class="pages"></ul>
      <div class="data"><h1>No data</h1></div>
      <ul class="pages"></ul>
    </div>
    <div class="tab" id="monomers">
      <ul class="pages"></ul>
      <div class="data"><h1>No data</h1></div>
      <ul class="pages"></ul>
    </div>
    <div class="tab" id="others">
      <ul class="pages"></ul>
      <div class="data"><h1>No data</h1></div>
      <ul class="pages"></ul>
    </div>
    <div class="tab" id="stats">
      <div class="data"><h1>No data</h1></div>
    </div>
  </body>

  <script src="lib/zip/zip.js"></script>
  <script src="lib/Chart-js/Chart.js"></script>
  <script src="js/Model.js"></script>
  <script src="js/Interface.js"></script>
  <script src="js/Loading.js"></script>
  <script src="js/ImageLoader.js"></script>
</html>
